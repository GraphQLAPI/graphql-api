{
    "name": "leoloso/graphql-api-wp-plugin",
    "type": "library",
    "description": "GraphQL API for WordPress",
    "keywords": [
        "PoP",
        "WordPress",
        "GraphQL",
        "API"
    ],
    "homepage": "https://github.com/leoloso/graphql-api-wp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.1",
        "getpop/api-rest": "~0.1",
        "getpop/block-metadata-for-wp": "~0.1",
        "getpop/commentmeta-wp": "~0.1",
        "getpop/comments-wp": "~0.1",
        "getpop/engine-wp": "~0.1",
        "getpop/graphql": "~0.1",
        "getpop/media-wp": "~0.1",
        "getpop/pages-wp": "~0.1",
        "getpop/postmedia-wp": "~0.1",
        "getpop/postmeta-wp": "~0.1",
        "getpop/posts-wp": "~0.1",
        "getpop/taxonomies-wp": "~0.1",
        "getpop/taxonomymeta-wp": "~0.1",
        "getpop/taxonomyquery-wp": "~0.1",
        "getpop/user-roles-access-control": "~0.1",
        "getpop/user-roles-wp": "~0.1",
        "getpop/user-state-wp": "~0.1",
        "getpop/usermeta-wp": "~0.1",
        "getpop/users-wp": "~0.1",
        "getpop/useful-directives": "~0.1",
        "getpop/api-endpoints-for-wp": "~0.1",
        "leoloso/pop-graphiql": "^1.3",
        "leoloso/pop-graphql-voyager": "^1.3",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leoloso\\GraphQLByPoPWPPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leoloso\\GraphQLByPoPWPPlugin\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "merge-plugin": {
            "include": [
                "composer.local.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "config": {
        "sort-packages": true
    }
}
